<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bigtennis.entity.dbEntity.ResultEntity" table="Results" schema="bigtennis">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="score">
            <column name="score" sql-type="varchar(3)" length="3"/>
        </property>
        <property name="set1">
            <column name="set1" sql-type="varchar(5)" length="5"/>
        </property>
        <property name="set2">
            <column name="set2" sql-type="varchar(5)" length="5"/>
        </property>
        <property name="set3">
            <column name="set3" sql-type="varchar(5)" length="5" not-null="true"/>
        </property>
        <property name="set4">
            <column name="set4" sql-type="varchar(5)" length="5" not-null="true"/>
        </property>
        <property name="set5">
            <column name="set5" sql-type="varchar(5)" length="5" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>