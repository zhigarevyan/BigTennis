<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.dbEntity.UsersEntity" table="Users" schema="bigtennis">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="key">
            <column name="key" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="role">
            <column name="role" sql-type="int"/>
        </property>
        <many-to-one name="userRolesByRole" class="entity.dbEntity.UserRolesEntity">
            <column name="role"/>
        </many-to-one>
    </class>
</hibernate-mapping>