<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="tabletennis.entity.dbEntity.MatchesUpdatesEntity" table="matchesUpdates" schema="bazabaka">
        <id name="id">
            <column name="id" sql-type="int(10) unsigned"/>
        </id>
        <property name="date">
            <column name="date" sql-type="varchar(17)" length="17"/>
        </property>
    </class>
</hibernate-mapping>